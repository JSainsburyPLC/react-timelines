{
  "name": "react-timelines",
  "version": "1.2.0",
  "description": "React Timelines",
  "main": "lib/index.js",
  "scripts": {
    "clean": "rimraf lib coverage",
    "build:js": "babel src/ -d lib/",
    "watch:js": "babel -w src/ -d lib/",
    "build:css": "node-sass src/scss/style.scss lib/css/style.css",
    "watch:css": "node-sass -w src/scss/style.scss lib/css/style.css",
    "watch": "yarn watch:js & yarn watch:css",
    "build": "yarn run clean && yarn build:js && yarn build:css",
    "test": "yarn lint && yarn unit",
    "lint:js": "eslint . --ext .js,.jsx",
    "lint": "yarn lint:js",
    "unit": "jest",
    "unit:ci": "jest --maxWorkers=4",
    "coverage": "jest --coverage --collectCoverageFrom='**/*.{js,jsx}'",
    "prepublish": "npm run clean && npm run build"
  },
  "repository": "git@github.com:JSainsburyPLC/react-timelines.git",
  "author": "JSainsburyPLC",
  "license": "MIT",
  "dependencies": {
    "prop-types": "^15.5.0"
  },
  "files": [
    "src",
    "lib"
  ],
  "devDependencies": {
    "babel-cli": "^6.24.0",
    "babel-core": "^6.24.0",
    "babel-eslint": "^7.2.1",
    "babel-jest": "^19.0.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.3.2",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-2": "^6.22.0",
    "enzyme": "^2.8.2",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.3",
    "jest": "^19.0.2",
    "node-sass": "^4.5.2",
    "react": "^15.4.2",
    "react-addons-test-utils": "^15.4.2",
    "react-dom": "^15.4.2",
    "rimraf": "^2.6.1"
  },
  "peerDependencies": {
    "react": ">=15.0",
    "react-dom": ">=15.0"
  },
  "jest": {
    "rootDir": "./src",
    "resetMocks": true,
    "resetModules": true,
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "<rootDir>/utils/raf.js",
      "<rootDir>/utils/events.js",
      "<rootDir>/utils/computedStyle.js",
      "<rootDir>/propTypes.js"
    ]
  }
}
